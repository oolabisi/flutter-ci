<?xml version="1.0" encoding="UTF-8"?>
<suite name="Suite" parallel="tests">
    <test name="iPhone14ProMax">
        <listeners>
            <listener class-name="com.qa.listeners.TestListener" />
        </listeners>
        <parameter name="platformName" value="iOS" />
        <parameter name="udid" value="D95A9686-EB41-4235-8E2C-5A8AFC556117" />
        <parameter name="deviceName" value="iPhone SE" />
        <parameter name="wdaLocalPort" value="10001" />
        <parameter name="webkitDebugProxyPort" value="11001" />
        <classes>
            <!-- LOGIN MODULE -->
            <class name="com.qa.tests.LoginTests">
                <methods>
                    <include name="validEmail"/>
                    <include name="validPassword"/>
                    <include name="modalPopUp"/>
                    <include name="signOut"/>
                </methods>
            </class>
        </classes>
    </test>
</suite> <!-- Suite -->
